import java.util.*;
public class fibonacci_series {
    public static void main(String[] args){
        Scanner sc= new Scanner(System.in);
        System.out.println("enter the n numbers of term ::-");
        int n = sc.nextInt();
        int a = 0 , b = 1;
        System.out.println("a"+n+"b");
        for( int i=1;i<n;i++){
            System.out.println(a+",");
            int c = a+b;
            a=b;
            b=c;
        }
    }
}
